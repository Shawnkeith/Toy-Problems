function map(arr, aFunction){
  var newArr = [];
    for(var i = 0; i < arr.length; i++){
     newArr.push((aFunction(arr[i])))
     console.log(newArr)
    }
  return newArr;
}


console.log(map([1,2,3],function doubler(element){
  return element*2;}))

  console.log(map(['a','b','c'],function addA(element){
  return element+'a';
})) //=> ['aa','ba','ca'])